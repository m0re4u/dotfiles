export ROBOLABWS="146.50.28.70"
export ROBOLABWS2="146.50.28.74"

if [ -z ${DNT_ROOT+x} ]; then
    echo "DNT_ROOT is unset, not loading dnt shortcuts";
else
    alias robocopy="${DNT_ROOT}/util/scripts/robocopy"
    alias dni="/${DNT_ROOT}/build/interface/dnt-interface"
    alias dnb="cd $DNT_ROOT"
    alias dnt="cd $(dirname $DNT_ROOT)"
fi

alias robossh="ssh mvdmeer@$ROBOLABWS"
alias robossh2="ssh mvdmeer@$ROBOLABWS2"


function _rnb() {
  ssh -N -f -L "localhost:$1:localhost:$2" "mvdmeer@$ROBOLABWS"
}
alias robonotebook=_rnb
